<p id="notice"><%= notice %></p>

<h1>Listing Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Access token</th>
      <th>Uid</th>
      <th>Photo url</th>
      <th>Provider</th>
      <th>Status</th>
      <th>Reason</th>
      <th>User type</th>
      <th>Data ingresso</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= usuario.name %></td>
        <td><%= usuario.email %></td>
        <td><%= usuario.access_token %></td>
        <td><%= usuario.uid %></td>
        <td><%= usuario.photo_url %></td>
        <td><%= usuario.provider %></td>
        <td><%= usuario.status %></td>
        <td><%= usuario.reason %></td>
        <td><%= usuario.user_type %></td>
        <td><%= usuario.data_ingresso %></td>
        <td><%= link_to 'Show', usuario %></td>
        <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
        <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario', new_usuario_path %>
